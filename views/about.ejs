              <ul class="sidebar-menu" id="nav-accordion">
                  <li class="sub-menu">
                      <a href="/profile">
                          <i class="fa fa-suitcase"></i>
                          <span> My Profile</span>
                      </a>
                  </li>

                    <li class="sub-menu">
                      <a href="/school/<%= school._id%>" >
                          <i class="fa fa-comments-o"></i>
                          <span> My School </span>
                      </a>
                     
                  </li>
                
                  <li class="sub-menu">
                      <a href="/schedule" >
                          <i class=" fa fa-th"></i>
                          <span> My Schedule</span>
                      </a>
                  </li>



              </ul>
              <!-- sidebar menu end-->
          </div>
      </aside>
      <!--sidebar end-->
